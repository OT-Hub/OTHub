    "@fortawesome/fontawesome-free": "^5.15.2",
    "@types/d3-color": "1.0.5",
    "@types/googlemaps": "^3.43.3",
    "@types/jasmine": "^3.6.4",
    "@types/jasminewd2": "^2.0.8",
    "@types/leaflet": "1.2.3",
    "@types/node": "^12.20.12",
    "codelyzer": "^6.0.0",
    "conventional-changelog-cli": "^2.1.1",
    "husky": "0.13.3",
